<html>
<head>
  <title> movie_App</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">>
</head>
<body> 
    <header class="jumbotron">
        <h1 class="text-center font-weight-bolder">results page!</h1>
          </header> 




         <h2 class ="text-center"><a href="/trailer/<%=data["id"]%>">TRAILER</a></h2>
          <div class="container">
          <div class="row" style="display:flex; flex-wrap:wrap;">
            

              
               


<%=data["overview"]%>

                <% data["cast"].forEach(function(movie){ %> 
                  <div class="col-md-4 col-sm-6">
                      <div class="thumbnail">
                    
                          <img src=<%="https:image.tmdb.org/t/p/w300/"+movie["profile_path"]%>>
                          <div class="caption">
                  <%= movie["name"]%>
                  </div>
                </div>
              </div>
                    
                <% }) %>
              </div>
            </div>
                </body>
                </html>
                