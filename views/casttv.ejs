<html>
<head>
  <title> movie_App</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">>
</head>
<body background="abstract-art-artistic-1020315.jpg"> 
    <header class="jumbotron">
        <h1 class="text-center font-weight-bolder">results!</h1>
          </header> 
          <div class="container">
          <div class="row" style="display:flex; flex-wrap:wrap;">

                <% data["cast"].forEach(function(movie){ %> 
                  <div class="col-lg-4 col-md-6">
                      <div class="thumbnail">
                          <img src=<%="https://image.tmdb.org/t/p/w300/"+movie["profile_path"]%>>
                          <div class="caption">
                 <h3><%= movie["name"]%></h3>
                <%=movie["character"]%>
                  </div>
                </div>
              </div>
                    
                <% }) %>
              </div>
            </div>
                </body>
                </html>
                