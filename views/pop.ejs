<head>
  <title> movie_App</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body> 
 
 <header class="jumbotron">
<h1 class="text-center font-weight-bolder">results page!</h1>
  </header> 
  <div class="container">
  <div class="row" style="display:flex; flex-wrap:wrap;">
<% data["results"].forEach(function(movie){ %> 
  <div class="col-md-4 col-sm-6">
    <div class="thumbnail">
      <a href="/results/<%=movie["id"]%>">
        <img src=<%="https:image.tmdb.org/t/p/w300/"+movie["poster_path"]%>>
        <div class="caption">
            <h3> <%=movie["title"]%></h3></a> 

        </div>
    </div>
  
  </div>
  <% }) %>
</div>
<h3 class="text-center">next page</h3>
<button><a href="/popularity/1">1</a></button>
<button><a href="/popularity/2">2</a></button>
<button><a href="/popularity/3">3</a></button>
<button><a href="/popularity/4">4</a></button>



  </div>


</body>
</html>